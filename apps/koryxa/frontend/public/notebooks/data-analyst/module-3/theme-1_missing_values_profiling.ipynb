      "source": [
        "# Module 3 — Thème 1\\n",
        "## Profilage & valeurs manquantes (preuve forte)\\n",
        "\\n",
        "Fichiers attendus en sortie :\\n",
        "- `m3t1_profiling_table.csv`\\n",
        "- `m3t1_dataset_clean.csv`\\n",
        "- `m3t1_quality_report.json`\\n",
        "- `m3t1_missingness_plan.md`\\n",
        "\\n",
        "Dataset d’entrée : `sales_users_messy.csv`\\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "import pandas as pd, json\\n",
        "from datetime import datetime\\n",
        "\\n",
        "df = pd.read_csv(\"sales_users_messy.csv\")\\n",
        "\\n",
        \"\"\"# ---------- helpers ----------\"\"\"\\n",
        "def to_clean_string(s: pd.Series) -> pd.Series:\\n",
        "    # preserve missing values (don't turn NaN into \"nan\")\\n",
        "    s = s.astype(\"string\")\\n",
        "    s = s.str.strip()\\n",
        "    s = s.str.replace(r\"\\\\s+\", \" \", regex=True)\\n",
        "    # convert empty strings to <NA>\\n",
        "    s = s.replace(\"\", pd.NA)\\n",
        "    return s\\n",
        "\\n",
        "def top_values(s: pd.Series, n=3):\\n",
        "    vc = s.dropna().astype(\"string\").value_counts().head(n)\\n",
        "    return {str(k): int(v) for k, v in vc.items()}\\n",
        "\\n",
        \"\"\"# ---------- 1) Profilage ----------\"\"\"\\n",
        "profiling_rows = []\\n",
        "for col in df.columns:\\n",
        "    s = df[col]\\n",
        "    na_pct = float(s.isna().mean() * 100)\\n",
        "    dtype = str(s.dtype)\\n",
        "    row = {\\n",
        "        \"column\": col,\\n",
        "        \"dtype\": dtype,\\n",
        "        \"na_pct\": round(na_pct, 2),\\n",
        "        \"n_unique\": int(s.nunique(dropna=True)),\\n",
        "        \"top3\": json.dumps(top_values(s, 3), ensure_ascii=False)\\n",
        "    }\\n",
        "\\n",
        "    # numeric / datetime ranges (best-effort)\\n",
        "    if col in [\"age\", \"revenue\"]:\\n",
        "        num = pd.to_numeric(s, errors=\"coerce\")\\n",
        "        row[\"min\"] = None if num.dropna().empty else float(num.min())\\n",
        "        row[\"max\"] = None if num.dropna().empty else float(num.max())\\n",
        "    if col in [\"signup_date\", \"last_active\"]:\\n",
        "        dt = pd.to_datetime(s, errors=\"coerce\", dayfirst=True, utc=True)\\n",
        "        row[\"min\"] = None if dt.dropna().empty else dt.min().isoformat()\\n",
        "        row[\"max\"] = None if dt.dropna().empty else dt.max().isoformat()\\n",
        "\\n",
        "    profiling_rows.append(row)\\n",
        "\\n",
        "profiling_df = pd.DataFrame(profiling_rows)\\n",
        "profiling_df.to_csv(\"m3t1_profiling_table.csv\", index=False)\\n",
        "\\n",
        \"\"\"# ---------- 2) Nettoyage minimal ----------\"\"\"\\n",
        "df[\"user_id\"] = to_clean_string(df[\"user_id\"])\\n",
        "df[\"country\"] = to_clean_string(df[\"country\"]).str.title()\\n",
        "df[\"channel\"] = to_clean_string(df[\"channel\"]).str.lower()\\n",
        "\\n",
        "df[\"signup_date\"] = pd.to_datetime(df[\"signup_date\"], errors=\"coerce\", dayfirst=True, utc=True)\\n",
        "df[\"last_active\"] = pd.to_datetime(df[\"last_active\"], errors=\"coerce\", dayfirst=True, utc=True)\\n",
        "\\n",
        "df[\"age\"] = pd.to_numeric(df[\"age\"], errors=\"coerce\")\\n",
        "df.loc[(df[\"age\"] < 10) | (df[\"age\"] > 90), \"age\"] = pd.NA\\n",
        "\\n",
        "df[\"revenue\"] = pd.to_numeric(df[\"revenue\"], errors=\"coerce\")\\n",
        "df.loc[df[\"revenue\"] < 0, \"revenue\"] = pd.NA\\n",
        "\\n",
        \"\"\"# ---------- 3) Flags missing ----------\"\"\"\\n",
        "for col in [\"country\",\"channel\",\"age\",\"signup_date\",\"last_active\",\"revenue\"]:\\n",
        "    df[f\"missing_{col}\"] = df[col].isna().astype(int)\\n",
        "\\n",
        \"\"\"# ---------- 4) Stratégies missingness ----------\"\"\"\\n",
        "df[\"country\"] = df[\"country\"].fillna(\"Unknown\")\\n",
        "df[\"channel\"] = df[\"channel\"].fillna(\"unknown\")\\n",
        "\\n",
        "age_median = float(df[\"age\"].median(skipna=True))\\n",
        "df[\"age\"] = df[\"age\"].fillna(age_median)\\n",
        "\\n",
        \"\"\"# revenue: keep NA + flag\"\"\"\\n",
        "\\n",
        \"\"\"# ---------- 5) Dédup contrôlée : garder la ligne la plus récente (éviter NaT) ----------\"\"\"\\n",
        "df = df.sort_values(\"last_active\", na_position=\"first\")\\n",
        "df = df.drop_duplicates(subset=[\"user_id\"], keep=\"last\").reset_index(drop=True)\\n",
        "\\n",
        "df.to_csv(\"m3t1_dataset_clean.csv\", index=False)\\n",
        "\\n",
        \"\"\"# ---------- 6) Rapport qualité ----------\"\"\"\\n",
        "quality = {\\n",
        "  \"created_at\": datetime.utcnow().isoformat()+\"Z\",\\n",
        "  \"rows\": int(len(df)),\\n",
        "  \"unique_user_id\": int(df[\"user_id\"].nunique()),\\n",
        "  \"duplicate_user_id\": int(len(df) - df[\"user_id\"].nunique()),\\n",
        "  \"missing_pct\": (df.isna().mean()*100).round(2).to_dict(),\\n",
        "  \"checks\": {\\n",
        "    \"user_id_unique\": bool(df[\"user_id\"].is_unique),\\n",
        "    \"age_reasonable\": bool(((df[\"age\"]>=10) & (df[\"age\"]<=90)).all()),\\n",
        "    \"dates_parsable\": bool(df[\"signup_date\"].notna().mean() >= 0.80)\\n",
        "  }\\n",
        "}\\n",
        "\\n",
        "with open(\"m3t1_quality_report.json\",\"w\",encoding=\"utf-8\") as f:\\n",
        "    json.dump(quality, f, ensure_ascii=False, indent=2)\\n",
        "\\n",
        \"\"\"# ---------- 7) Plan missingness ----------\"\"\"\\n",
        "plan_md = f\"\"\"# Missingness Plan — Module 3 Theme 1\\n",
        "\\n",
        "## Décisions\\n",
        "- age: médiane={age_median:.2f} + flag missing_age\\n",
        "- country: 'Unknown' + flag missing_country\\n",
        "- channel: 'unknown' + flag missing_channel\\n",
        "- revenue: gardé NA + flag missing_revenue (risque MNAR possible)\\n",
        "\\n",
        "## Notes\\n",
        "- Dédup: 1 ligne par user_id, conservation du last_active le plus récent.\\n",
        "\"\"\"\\n",
        "open(\"m3t1_missingness_plan.md\",\"w\",encoding=\"utf-8\").write(plan_md)\\n",
        "\\n",
        "print(\"✅ Exports generated: profiling_table / dataset_clean / quality_report / missingness_plan\")\\n"
      ]
    }
  ],
